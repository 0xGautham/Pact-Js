{
  "consumer": {
    "name": "Consumer Example"
  },
  "interactions": [
    {
      "description": "a request for all users",
      "providerState": "Has some users",
      "request": {
        "headers": {
          "Authorization": "Bearer token"
        },
        "matchingRules": {
          "$.header.$": {
            "match": "type"
          }
        },
        "method": "GET",
        "path": "/users"
      },
      "response": {
        "body": [
          {
            "accountNumber": "1234567890",
            "balance": 5000,
            "id": 1,
            "name": "John Doe"
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body[0].accountNumber": {
            "match": "type"
          },
          "$.body[0].balance": {
            "match": "type"
          },
          "$.body[0].id": {
            "match": "type"
          },
          "$.body[0].name": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request for all users",
      "providerState": "is not authenticated",
      "request": {
        "method": "GET",
        "path": "/users"
      },
      "response": {
        "status": 401
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "11.0.2"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "Provider Example"
  }
}